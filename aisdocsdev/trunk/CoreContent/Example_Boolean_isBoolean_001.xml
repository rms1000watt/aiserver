<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE Example SYSTEM "../DTD/Example.dtd">
<Example>
	<KnowledgeBase>
		<Title>Example Boolean isBoolean 001</Title>
		<Template>Example</Template>
		<FunctionKeywords>isBoolean</FunctionKeywords>
		<TypeKeywords>Boolean  Integer</TypeKeywords>
		<RelatedFunctions>isType isObject</RelatedFunctions>
		<Language>Lisp</Language>
		<HumanKeywords>  bit byte   Boolean</HumanKeywords>
	</KnowledgeBase>
	<Description><![CDATA[ This example checks if the object evaluated is a Boolean

                  

                  If it is, the function returns true, otherwise, it returns false.




	]]></Description>
	<Syntax>
  	    <Expression><![CDATA[ <sc>(isBoolean  -1)           </sc>]]></Expression>
        <Note><![CDATA[none]]></Note>
        <Returns><![CDATA[false]]></Returns>
  	    <Expression><![CDATA[ <sc>(isBoolean  '(1  .  2))             </sc>]]></Expression>  
        <Note><![CDATA[none]]></Note> 
        <Returns><![CDATA[false]]></Returns>
 	    <Expression><![CDATA[ <sc>(isBoolean  false)             </sc>]]></Expression>  
        <Note><![CDATA[none]]></Note> 
        <Returns><![CDATA[true]]></Returns>
	    <Expression><![CDATA[ <sc>(isBoolean  (= 2  3))        </sc>]]></Expression>  
        <Note><![CDATA[none]]></Note> 
        <Returns><![CDATA[true]]></Returns>
        <Hints><![CDATA[none]]></Hints>
   </Syntax>
</Example>
