<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE Example SYSTEM "../DTD/Example.dtd">
<Example>
	<KnowledgeBase>
		<Title>Example String replace 001</Title>
		<Template>Example</Template>
		<FunctionKeywords>replace</FunctionKeywords>
		<TypeKeywords>String Text Integer</TypeKeywords>
		<RelatedFunctions>substitute rept </RelatedFunctions>
		<Language>Lisp</Language>
		<HumanKeywords>replace substitute substring</HumanKeywords>
	</KnowledgeBase>
	<Description><![CDATA[ This example shows the <b>replace</b> function modifying
                  the characters of the original String with the new
                  Text or String at the specified position.
	]]></Description>
	<Syntax>
  	    <Expression><![CDATA[ <sc>(setq S  (new String:  "Small World"))      </sc>]]></Expression>
        <Note><![CDATA[none]]></Note>
        <Returns><![CDATA[Small World]]></Returns>
        <Expression><![CDATA[ <sc>(setq S (replace S 0  5 "Old"))</sc>]]></Expression>
        <Note><![CDATA[none ]]></Note>
        <Returns><![CDATA[ Old World ]]></Returns>
        <Hints><![CDATA[none]]></Hints>
   </Syntax>
</Example>
