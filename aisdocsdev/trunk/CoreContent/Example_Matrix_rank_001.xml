<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE Example SYSTEM "../DTD/Example.dtd">
<Example>
	<KnowledgeBase>
		<Title>Example Matrix rank 001</Title>
		<Template>Example</Template>
		<FunctionKeywords>rank</FunctionKeywords>
		<TypeKeywords>Matrix  Integer</TypeKeywords>
		<RelatedFunctions>new  objectToNumMatrix objectToMatrix</RelatedFunctions>
		<Language>Lisp</Language>
		<HumanKeywords> rank dimension</HumanKeywords>
	</KnowledgeBase>
	<Description><![CDATA[ This example returns the number of values in the Matrix using the <b>rank</b> function.




	]]></Description>
	<Syntax>
  	     <Expression><![CDATA[ <sc>(rank  #(mat| "Mary" #Feb,2,1980) )          </sc>]]></Expression>
        <Note><![CDATA[none]]></Note>
        <Returns><![CDATA[#&#60Matrix 123456&#62]]></Returns>
        <Expression><![CDATA[ <sc>(display #&#60Matrix 123456&#62)      </sc>]]></Expression>
        <Note><![CDATA[none ]]></Note>
        <Returns><![CDATA[ #(1) ]]></Returns>
        <Expression><![CDATA[ <sc>(rank  #(mat[2 3]| 4  5  6  7  9  3) )     </sc>]]></Expression>
        <Note><![CDATA[none]]></Note>
        <Returns><![CDATA[#&#60Matrix 234567&#62]]></Returns>
        <Expression><![CDATA[ <sc>(display #&#60Matrix 234567&#62)      </sc>]]></Expression>  
        <Note><![CDATA[none ]]></Note> 
        <Returns><![CDATA[ #(2 3) ]]></Returns>
        <Hints><![CDATA[none]]></Hints>
   </Syntax>
</Example>
