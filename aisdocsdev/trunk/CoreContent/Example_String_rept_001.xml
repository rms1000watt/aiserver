<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE Example SYSTEM "../DTD/Example.dtd">
<Example>
	<KnowledgeBase>
		<Title>Example String rept 001</Title>
		<Template>Example</Template>
		<FunctionKeywords>rept</FunctionKeywords>
		<TypeKeywords>String Text Integer</TypeKeywords>
		<RelatedFunctions>substitute replace </RelatedFunctions>
		<Language>Lisp</Language>
		<HumanKeywords>replace substitute substring repeating</HumanKeywords>
	</KnowledgeBase>
	<Description><![CDATA[ This example shows the <b>rept</b> function creating a new String
                  out of the characters of the original String.
	]]></Description>
	<Syntax>
  	    <Expression><![CDATA[ <sc>(setq S  (new String:  "Small World") )    </sc>]]></Expression>
        <Note><![CDATA[none]]></Note>
        <Returns><![CDATA[Small World]]></Returns>
        <Expression><![CDATA[ <sc>(rept S 2)</sc>]]></Expression>
        <Note><![CDATA[none ]]></Note>
        <Returns><![CDATA[Small WorldSmall World]]></Returns>
        <Expression><![CDATA[ <sc>(rept S 3)</sc>]]></Expression>  
        <Note><![CDATA[none ]]></Note> 
        <Returns><![CDATA[Small WorldSmall WorldSmall World]]></Returns>
        <Hints><![CDATA[none]]></Hints>
   </Syntax>
</Example>
