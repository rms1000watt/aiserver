<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE Example SYSTEM "../DTD/Example.dtd">
<Example>
	<KnowledgeBase>
		<Title>Example Number sign 001</Title>
		<Template>Example</Template>
		<FunctionKeywords>sign</FunctionKeywords>
		<TypeKeywords>Number Integer</TypeKeywords>
		<RelatedFunctions>  isPositive isNegative</RelatedFunctions>
		<Language>Lisp</Language>
		<HumanKeywords>  positive negative sign </HumanKeywords>
	</KnowledgeBase>
	<Description><![CDATA[ These examples use the <b>sign</b> function to return <b>1</b> if the
                  number argument is <b>positive</b> or <b>-1</b> if the
                 number argument is <b>negative</b>.
	]]></Description>
	<Syntax>
  	    <Expression><![CDATA[ <sc>(sign -227.2)  </sc>]]></Expression>
        <Note><![CDATA[none]]></Note>
        <Returns><![CDATA[-1.0]]></Returns>
  	    <Expression><![CDATA[ <sc>(sign 23.5)  </sc>]]></Expression>  
        <Note><![CDATA[none]]></Note> 
        <Returns><![CDATA[1.0]]></Returns>
  	    <Expression><![CDATA[ <sc>(sign 0)  </sc>]]></Expression>  
        <Note><![CDATA[none]]></Note> 
        <Returns><![CDATA[0.0]]></Returns>
        <Hints><![CDATA[none]]></Hints>
   </Syntax>
</Example>
