<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE Example SYSTEM "../DTD/Example.dtd">
<Example>
	<KnowledgeBase>
		<Title>Example String upcase 001</Title>
		<Template>Example</Template>
		<FunctionKeywords>upcase</FunctionKeywords>
		<TypeKeywords>String Text Character</TypeKeywords>
		<RelatedFunctions>downcase</RelatedFunctions>
		<Language>Lisp</Language>
		<HumanKeywords>upcase downcase character</HumanKeywords>
	</KnowledgeBase>
	<Description><![CDATA[ This example converts all lowercase characters of the specified Text into

                  uppercase characters.  

	]]></Description>
	<Syntax>
  	    <Expression><![CDATA[ <sc>(define S "Mary") </sc>]]></Expression>
        <Note><![CDATA[Defines a Text ]]></Note>
        <Returns><![CDATA[Mary]]></Returns>
        <Expression><![CDATA[ <sc>(setq T (upcase S))</sc>]]></Expression>
        <Note><![CDATA[none ]]></Note>
        <Returns><![CDATA[ MARY ]]></Returns>
        <Expression><![CDATA[ <sc>(display S)</sc>]]></Expression>
        <Note><![CDATA[none ]]></Note>
        <Returns><![CDATA[Mary]]></Returns>
        <Expression><![CDATA[ <sc>(display T)</sc>]]></Expression>
        <Note><![CDATA[none ]]></Note>
        <Returns><![CDATA[  MARY  ]]></Returns>
        <Hints><![CDATA[The original Text argument is not destroyed.
        ]]></Hints>
   </Syntax>
</Example>
