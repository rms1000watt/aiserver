<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE Example SYSTEM "../DTD/Example.dtd">
<Example>
	<KnowledgeBase>
		<Title>Example Complex conjugate 001</Title>
		<Template>Example</Template>
		<FunctionKeywords>conjugate</FunctionKeywords>
		<TypeKeywords>Complex</TypeKeywords>
		<RelatedFunctions>complex imaginary  </RelatedFunctions>
		<Language>Lisp</Language>
		<HumanKeywords>negates nagative imaginary </HumanKeywords>
	</KnowledgeBase>
	<Description><![CDATA[  <p> The conjugate of a complex value is shown below. The operation 
              is NOT destructive.</p>
	]]></Description>
	<Syntax>
  	    <Expression><![CDATA[ <sc>
(setq X (new Complex: 1.0 2.0))   </sc>   ]]></Expression>
        <Note><![CDATA[none]]></Note>
        <Returns><![CDATA[#&#60Complex 123456&#62]]></Returns>
  	    <Expression><![CDATA[ <sc>(conjugate X)   </sc>   ]]></Expression>  
        <Note><![CDATA[none]]></Note> 
        <Returns><![CDATA[#&#60Complex 123456&#62]]></Returns>
  	    <Expression><![CDATA[ <sc>(display X)   </sc>   ]]></Expression>  
        <Note><![CDATA[none]]></Note> 
        <Returns><![CDATA[#c1.0-2.0i]]></Returns>
        <Hints><![CDATA[none
        ]]></Hints>
   </Syntax>
</Example>
