<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE Function SYSTEM "../DTD/Function.dtd">
<Function>
	<KnowledgeBase>
        <Title>rad</Title>
		<FunctionName>rad</FunctionName>
		<FunctionType>Builtin(Trig)</FunctionType>
		<ArgumentTypes> Number  </ArgumentTypes>
		<DataType>Number  </DataType>
		<RelatedFunctions> deg </RelatedFunctions>
        <RelatedVMInstructions>none</RelatedVMInstructions>
		<HumanKeywords> radians degree</HumanKeywords>
	</KnowledgeBase>
         <Description><![CDATA[
             <p> The <b>rad</b> function converts a value specified in degrees to radians.</p>
          ]]></Description>
	<Brief>
	   <Overview><![CDATA[
	       <p> The <b>rad</b> function converts a value specified in degrees to radians.</p>
        ]]></Overview>
        <Usage><![CDATA[
     	      <p> The <b>rad</b> function is used to convert a number expressed in degrees 
              to a number expressed in radians.</p>
        ]]></Usage>
    </Brief>
	<Syntax>
	    <Expression><![CDATA[<p><b> (rad num)</b></p>]]></Expression>
		<Argument>
		  <Name>num</Name>
		  <Note><![CDATA[
		  <p>A value expressed in degrees.</p>
          ]]></Note>
		  <AISTypes>Number</AISTypes>
		</Argument>
		<Return><![CDATA[
	    <p>The value {num} expressed in radians.</p>
	    ]]></Return>
	</Syntax>
</Function>
